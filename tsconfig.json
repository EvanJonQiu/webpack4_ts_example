{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@app/*": ["./src/*"],
      "tslib": ["./node_modules/tslib/tslib.d.ts"]
    }
  },
  "include":[
    "./src/**/*",
    "./src/images.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}